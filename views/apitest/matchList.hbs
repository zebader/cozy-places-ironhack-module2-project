<!-- views/apitest/matchList.hbs -->
<div class='list-container'>
      <h2 class='places-title'>Match LIST</h2>
      {{#if finalVenues.isSearchA}}
      {{#each finalVenues.venuesIDarrayA}}
            <div class="card-wrapper">

                  <button type="submit">
                        <div class='card'>
                              <img src={{this.imgUrl}}>
                              <div class="card-info">
                                    <h4>{{this.name}}</h4>
                                    <p>{{this.location.address}}</p>
                                    <p>{{this.location.city}}</p>
                              </div>
                        </div>
                  </button>

            <form action="/matchtest/new" method="GET">
                  <input type="hidden" value='{{this.imgUrl}}' name='imgUrl'>
                  <input type="hidden" value='{{this.name}}' name='name'>
                  <input type="hidden" value='{{this.location.address}}' name='address'>
                  <input type="hidden" value='{{this.location.city}}' name='city'>

                  <input type="hidden" value='{{../finalVenues.venuesIDarrayB.img}}' name='venuesIDarrayImg'>
                  <input type="hidden" value='{{../finalVenues.venuesIDarrayB.venuesIDarray.name}}' name='venuesIDarrayName'>
                  <input type="hidden" value='{{../finalVenues.venuesIDarrayB.venuesIDarray.location}}' name='venuesIDarrayAddress'>
                  <input type="hidden" value='{{../finalVenues.venuesIDarrayB.venuesIDarray.city}}' name='venuesIDarrayCity'>

                  <button type="submit" class="button add-place-button">MATCH</button>
            </form>

            </div>

      {{/each}}
      {{/if}}
      {{#if finalVenues.isSearchB}}
      {{#each finalVenues.venuesIDarrayB}}
      <div class="card-wrapper">

            <button type="submit">
                  <div class='card'>
                        <img src={{this.imgUrl}}>
                        <div class="card-info">
                              <h4>{{this.name}}</h4>
                              <p>{{this.location.address}}</p>
                              <p>{{this.location.city}}</p>
                        </div>
                  </div>
            </button>

      <form action="/matchtest/newB" method="GET">
            <input type="hidden" value='{{this.imgUrl}}' name='imgUrl'>
            <input type="hidden" value='{{this.name}}' name='name'>
            <input type="hidden" value='{{this.location.address}}' name='address'>
            <input type="hidden" value='{{this.location.city}}' name='city'>

            <input type="hidden" value='{{../finalVenues.venuesIDarrayA.img}}' name='venuesIDarrayImg'>
            <input type="hidden" value='{{../finalVenues.venuesIDarrayA.name}}' name='venuesIDarrayName'>
            <input type="hidden" value='{{../finalVenues.venuesIDarrayA.location}}' name='venuesIDarrayAddress'>
            <input type="hidden" value='{{../finalVenues.venuesIDarrayA.city}}' name='venuesIDarrayCity'>

            <button type="submit" class="button add-place-button">MATCH</button>
      </form>

      </div>

      {{/each}}
      {{/if}}
</div>